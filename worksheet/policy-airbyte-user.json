        "Action": [
          "s3:PutObject",
          "s3:GetObject",
          "s3:DeleteObject",
          "s3:PutObjectAcl",
          "s3:ListBucket",
          "s3:ListBucketMultipartUploads",
          "s3:AbortMultipartUpload",
          "s3:GetBucketLocation"
        ],
        "Resource": [
          "arn:aws:s3:::prd-datalake-team-c/*",
          "arn:aws:s3:::prd-datalake-team-c"
        ]
      }
    ]
  }